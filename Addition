# include <iostream>
using namespace std;

int main(){
    int num1;
    int num2;

    cout<< "Enter first number:\n";
    cin>>num1;

    cout<< "Enter second number:\n";
    cin>>num2;

    cout<< "The sum is: "<< num1 +num2;


    // '<<' is Insertion operator
    //'>>' is Extraction operator
    return 0;
}
